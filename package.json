{
  "name": "express-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.js",
  "scripts": {
    "test": "mocha --require test/setup.js",
    "dev": "nodemon src/server.js",
    "migrate": "postgrator --config postgrator-config.js",
    "migrate:test": "env NODE_ENV=test npm run migrate",
    "migrate:prod": "env SSL=true DATABASE_URL=$(heroku config:get DATABASE_URL) npm run migrate",
    "migrate:prod0": "env SSL=true DATABASE_URL=$(heroku config:get DATABASE_URL) npm run migrate -- 0",
    "start": "node src/server.js",
    "predeploy": "npm audit",
    "deploy": "git push heroku master",
    "seed": "psql -U postgres -d mystripes -f ./db/seeds/seed.all_tables.sql"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.2",
    "morgan": "^1.9.1",
    "nodemailer": "^6.4.11",
    "pg": "^7.8.0",
    "stripe": "^8.84.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^7.2.0",
    "nodemon": "^2.0.4",
    "postgrator-cli": "^3.2.0",
    "supertest": "^4.0.2"
  }
}
